{
    "name": "n8n-nodes-quick-jira",
    "version": "0.1.2",
    "description": "Simplified Jira node for n8n with smart defaults and common operations - create tickets, update status, add comments, and search with ease",
    "license": "MIT",
    "homepage": "https://github.com/tonyjames/n8n-nodes-quick-jira",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "jira",
        "atlassian",
        "project management",
        "simplified"
    ],
    "author": {
        "name": "Tony James",
        "email": "me@tony.codes"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tonyjames/n8n-nodes-quick-jira.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/QuickJiraApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/QuickJira/QuickJira.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
